{
  "name": "sovereigntyos-ai",
  "version": "1.0.0",
  "description": "Enterprise AI-Native Operating System with Enhanced Cognitive Architecture",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.{ts,tsx} --fix",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "type-check": "tsc --noEmit",
    "deploy": "npm run build && npm run preview",
    "docker:build": "docker build -t sovereigntyos-ai .",
    "docker:run": "docker run -p 3000:80 sovereigntyos-ai",
    "cognitive:init": "node scripts/init-cognitive-architecture.js",
    "cognitive:train": "node scripts/train-cognitive-models.js"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.0.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "axios": "^1.5.0",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0",
    "ws": "^8.14.0",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.3",
    "vite": "^4.4.5",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "@types/lodash": "^4.14.198",
    "@types/uuid": "^9.0.4",
    "@types/ws": "^8.5.5",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.3",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.1.3",
    "jsdom": "^22.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "ai",
    "cognitive-architecture",
    "sovereignty",
    "workflow-automation",
    "enterprise",
    "south-africa",
    "typescript",
    "react",
    "docker"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/deedk822-lang/sovereigntyos_AI.git"
  },
  "author": "deedk822-lang",
  "license": "MIT"
}